{
  "name": "depay-web3mock",
  "moduleName": "Web3Mock",
  "version": "1.0.2",
  "description": "JavaScript library to mock web3 responses either by emulating blockchain wallets or RPC calls.",
  "main": "./dist/cjs/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier src --write",
    "prepublishOnly": "yarn build",
    "test": "yarn build && npx jest --no-cache --forceExit"
  },
  "repository": "git@github.com:DePayFi/depay-web3mock.git",
  "keywords": [
    "web3",
    "mock",
    "testing",
    "blockchain",
    "wallets",
    "crypto"
  ],
  "author": "depay.fi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DePayFi/depay-web3mock/issues"
  },
  "homepage": "https://github.com/DePayFi/depay-web3mock#readme",
  "private": false,
  "dependencies": {
    "ethers": "^5.3.1"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-sucrase": "^3.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.34.2"
  }
}
